{
  "name": "generator-np-stack",
  "version": "0.0.1",
  "description": "Generator NP Stack",
  "homepage": "https://noprotocol.nl",
  "author": {
    "name": "Govert Verschuur",
    "email": "govert@noprotocol.nl",
    "url": "https://noprotocol.nl"
  },
  "contributors": [
    "Stef van den Ham <stef@mindthecode.com> (https://mindthecode.com/)",
    "Anne Jan Brouwer <brouwer@annejan.com> (https://annejan.com/)",
    "Bob Fanger <info@bfanger.nl> (https://bfanger.nl/)"
  ],
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^4.1.0",
    "eslint-config-recommended": "^2.0.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "@vue/eslint-config-prettier": "^3.0.0-beta.6",
    "chalk": "^2.1.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-vue": "^4.3.0",
    "fs-extra": "^5.0.0",
    "replace-in-file": "^3.2.0",
    "slugify": "^1.2.9",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest ./__tests__"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/prettier"
    ],
    "plugins": [
      "vue",
      "only-warn"
    ],
    "rules": {
      "vue/max-attributes-per-line": "off",
      "import/no-unresolved": "off",
      "import/no-commonjs": "off",
      "import/no-nodejs-modules": "off"
    }
  },
  "repository": "noprotocol/generator-np-stack",
  "license": "MIT"
}
